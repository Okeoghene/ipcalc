subnet_mask = '255.255.255.0' #currently acting as the subnet_mask input

subnet_mask = subnet_mask.split('.') #This converts the input to a list

mask_holder = [] #creates an empty list.

for digits in subnet_mask:
    """
    This converts the list to the values in the list to integers
    It also converts the intergers to binary and removes the '0b' 
    signature for binary
    """
    digits = int(digits)
    digits = bin(digits).replace('0b','')
    mask_holder.append(digits)

subnet_mask = ''.join(mask_holder) #Converts the list to a string

print(subnet_mask.count('1')) #prints the number of the 1's in the binary
print(len(subnet_mask))